{
	"manifest_version": 3,
	"name": "AITransform",
	"version": "1.0",
	"description": "",
	"permissions": ["storage", "activeTab", "scripting"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icons/icon16.png"
		}
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"js": ["content.js"],
			"resources": ["summary.html"],
			"matches": ["<all_urls>"]
		}
	]
}
